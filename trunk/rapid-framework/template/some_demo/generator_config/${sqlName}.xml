<?xml version="1.0" encoding="UTF-8"?>
<#assign className = table.className>   
<#assign classNameLower = className?uncap_first> 
<!--******************************
 强烈提醒: 如果你不想自定义配置,可以删除整个文件或是其中几列, 有定义的列才读自定义配置,没有定义的读数据库.不用一一对应
 ***********************************-->
 
<!-- 
<数据库表名 className="类名称" tableAlias="表的别名">
	<数据库列名 columnAlias="列的别名" 
	javaType="自定义javaType"
	unique="是否唯一性约束" nullable="是否可以为空" pk="是否主键,在表没有主键的情况下,可以指定一个代理主键" 
	updatable="是否可以更新" insertable="是否插入"	
	enumMapString="枚举值,格式为键值对,并以逗号分隔,如:k1="str",k2=30,k3=40" 
	enumClassName="如果枚举有值,生成的类名称将是这个,没有枚举值，该配置无用"
	/>
</数据库表名>
 -->
<${table.sqlName} className="${className}" tableAlias="${table.tableAlias}" >
<#list table.notPkColumns as c>
	<${c.sqlName!} columnAlias="${c.columnAlias!}"
		javaType="${c.simpleJavaType!}" 
		unique="${c.unique?string}" nullable="${c.nullable?string}" pk="${c.pk?string}"
		updatable="${c.updatable?string}" insertable="${c.insertable?string}"
		enumMapString="${c.enumMapString!}" enumClassName="${c.enumClassName!}"
	/>
</#list>
</${table.sqlName}>
