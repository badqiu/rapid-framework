
<#if (sql.params?size > 4) >
	public <@generateResultClassName/> ${sql.operation}(${sql.operation}Query query) {
		<@generateOperationMethodBody />
	}
<#else>
	public <@generateResultClassName/> ${sql.operation}(<#list sql.params as param>${param.parameterClassName} ${param.paramName} <#if param_has_next>,</#if></#list>) {
		<@generateOperationMethodBody />
	}
</#if>

<#macro generateResultClassName>
<#compress>
	<#if sql.multiPolicy = 'one'>
		${sql.queryResultClassName}
	<#else>
		List<${sql.queryResultClassName}>
	</#if>
</#compress>
</#macro>

<#macro generateOperationMethodBody>
		<#if sql.multiPolicy = 'one'>
			return (<@generateResultClassName/>)queryForObject("${sql.sourceSql}");
		<#else>
			return (<@generateResultClassName/>)queryForList("${sql.sourceSql}");
		</#if>
</#macro>